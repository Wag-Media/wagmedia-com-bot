    "discord.js": "^14.7.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^3.12.2",
    "typescript": "^5.3.3",
    "undici": ">=5.28.3",
    "viem": "^2.0.3"
  },
