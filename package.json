    "@types/node": "^20.10.7",
    "corde": "^4.8.0",
    "prisma": "^5.9.1",
    "simple-git-hooks": "^2.11.0",
    "ts-jest": "^29.1.1",
    "tsconfig-paths-jest": "^0.0.1"
